  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.1",
    "chai": "^4.3.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jsdom": "16.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.1",
    "prettier": "^2.2.1",
